<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="photonyaWindow" parent="GtkApplicationWindow">
    <property name="title">photonya</property>
      <child>
        <object class="GtkFileChooserDialog" id="file_chooser_dialog"/>
      </child>
      <child>
        <object class="GtkBox" id="box">
          <property name="margin-top">12</property>
          <property name="margin-bottom">12</property>
          <property name="margin-start">12</property>
          <property name="margin-end">12</property>
          <property name="orientation">vertical</property>
          <child>
            <object class="GtkLabel" id="label">
              <property name="margin-top">12</property>
              <property name="margin-bottom">12</property>
              <property name="margin-start">12</property>
              <property name="margin-end">12</property>
              <property name="label">Choose a file</property>
            </object>
          </child>
          <child>
            <object class="GtkButton" id="file_chooser">
              <property name="margin-top">12</property>
              <property name="margin-bottom">12</property>
              <property name="margin-start">12</property>
              <property name="margin-end">12</property>
              <property name="label">Choose a file</property>
            </object>
          </child>
          <child>
            <object class="GtkPicture" id="picture">
              <property name="margin-top">12</property>
              <property name="margin-bottom">12</property>
              <property name="margin-start">12</property>
              <property name="margin-end">12</property>  
            </object>
          </child>
        </object>
      </child>
  </template>
</interface>